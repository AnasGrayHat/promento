     <artifactId>jetty-maven-plugin</artifactId>
     <version>9.3.7.v20160115</version>
   </plugin>
   
   <plugin>
    <groupId>com.google.appengine</groupId>
    <artifactId>gcloud-maven-plugin</artifactId>
    <version>1.9.48</version>
    <configuration>
        <set_default>true</set_default>
    </configuration>
</plugin>



   
		</plugins>
		

		
		
		
		
	</build>


